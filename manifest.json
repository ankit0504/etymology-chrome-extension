{
  "manifest_version": 2,
  "name": "Joey Tribbiani Thesaurus",
  "version": "1.0",
  "description": "Mass use a thesaurus, just like Joey Tribbiani",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.3.1.min.js" ,"bundle.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "default_title": "Joey Tribbiani Thesaurus"
  }
}
