{
  "manifest_version": 2,
  "name": "Thesaurus Chrome Extension",
  "version": "1.0",
  "description": "Easily discover the etymology of a word",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.3.1.min.js" ,"bundle.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
